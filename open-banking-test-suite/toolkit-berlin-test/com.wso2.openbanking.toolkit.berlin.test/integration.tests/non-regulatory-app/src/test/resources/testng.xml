<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Berlin Group Integration">
    <test name="Non Regulatory Application Flow Tests" parallel="false" enabled="true">
        <classes>
            <class name="com.wso2.openbanking.toolkit.berlin.integration.test.nonregulatoryapp.Non_Regulatory_Common_Tests.EndToEndFlowWithAuthorizationCodeGrantTypeTest"/>
            <class name="com.wso2.openbanking.toolkit.berlin.integration.test.nonregulatoryapp.Non_Regulatory_Common_Tests.EndToEndFlowWithClientCredentialsGrantTypeTest"/>
            <class name="com.wso2.openbanking.toolkit.berlin.integration.test.nonregulatoryapp.Non_Regulatory_Common_Tests.EndToEndFlowWithPasswordGrantTypeTest"/>
        </classes>
    </test>
    <test name="Non Regulatory Application - Token Flow Tests" parallel="false" enabled="true">
        <classes>
            <class name="com.wso2.openbanking.toolkit.berlin.integration.test.nonregulatoryapp.Non_Regulatory_Token_Generation_Tests.TokenGenWithAuthorizationCodeValidationTests"/>
            <class name="com.wso2.openbanking.toolkit.berlin.integration.test.nonregulatoryapp.Non_Regulatory_Token_Generation_Tests.TokenGenWithClientCredentialsValidationTests"/>
            <class name="com.wso2.openbanking.toolkit.berlin.integration.test.nonregulatoryapp.Non_Regulatory_Token_Generation_Tests.TokenGenWithPasswordGrantValidationTests"/>
        </classes>
    </test>
</suite>
